<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="images\bouton_play.png" />
    <None Remove="images\pause.png" />
    <None Remove="images\sol.jpg" />
    <None Remove="images\souris.png" />
    <None Remove="images\souris_etourdie.png" />
    <None Remove="images\touche.jpg" />
    <None Remove="images\trou.png" />
    <None Remove="MusiqueNiveau1.mp3" />
    <None Remove="Musiques\MusiqueNiveau1.mp3" />
    <None Remove="Musiques\touche.mp3" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="images\bouton_play.png" />
    <Resource Include="images\pause.png" />
    <Resource Include="images\sol.jpg" />
    <Resource Include="images\souris.png" />
    <Resource Include="images\souris_etourdie.png" />
    <Resource Include="images\trou.png" />
    <Content Include="Musiques\MusiqueNiveau1.mp3">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

</Project>
